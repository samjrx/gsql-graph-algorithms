USE GRAPH polblog
DROP JOB  load_polblog

CREATE LOADING JOB load_polblog FOR GRAPH polblog {
    DEFINE FILENAME vfile;
    DEFINE FILENAME efile;

    LOAD vfile
        TO VERTEX Blog VALUES ($0, $1, $2, $3, _)
        USING header="true";
    LOAD efile
        TO EDGE Cited VALUES ($0, $1, 1)
        USING header="true";
        
}
#RUN LOADING JOB load_polblog USING dir_efile="./data/data.csv"
