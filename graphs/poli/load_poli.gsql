USE GRAPH poli
DROP JOB  load_poli

CREATE LOADING JOB load_poli FOR GRAPH poli {
    DEFINE FILENAME efile;

    LOAD efile
        TO VERTEX Account VALUES ($0, $0, _),
        TO EDGE Link_to VALUES ($0, $1, $2, _)
        USING header="true";
        
}
#RUN LOADING JOB load_poli USING dir_efile="./data/data.csv"
