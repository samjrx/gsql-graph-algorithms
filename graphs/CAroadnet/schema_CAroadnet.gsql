USE GRAPH CAroadnet
//DROP QUERY ALL
DROP JOB ALL
DROP GRAPH CAroadnet

CREATE GRAPH CAroadnet ()
CREATE SCHEMA_CHANGE JOB schema_CAroadnet FOR GRAPH CAroadnet {
    ADD VERTEX Intersection (PRIMARY_ID id STRING, ID STRING);
    ADD UNDIRECTED EDGE Road (FROM Intersection, TO Intersection, weight FLOAT);
}