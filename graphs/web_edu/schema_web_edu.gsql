USE GRAPH web_edu
DROP QUERY ALL
DROP JOB ALL
DROP GRAPH web_edu

CREATE GRAPH web_edu ()
CREATE SCHEMA_CHANGE JOB schema_web_edu FOR GRAPH web_edu {
    ADD VERTEX Webpage (PRIMARY_ID id STRING, ID STRING);
    ADD DIRECTED EDGE Hyperlink_to (FROM Webpage, TO Webpage, weight FLOAT) WITH REVERSE_EDGE="Hyperlink_from";
}