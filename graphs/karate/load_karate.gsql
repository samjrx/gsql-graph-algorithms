USE GRAPH karate
DROP JOB load_karate

CREATE LOADING JOB load_karate FOR GRAPH karate {
    DEFINE FILENAME vfile;
    DEFINE FILENAME efile;

    LOAD vfile
        TO VERTEX Member VALUES ($1, $1, $2, $0)
        USING header="true";
    LOAD efile
        TO EDGE Interact VALUES ($0, $1, $2)
        USING header="true";
        
}