USE GRAPH USairport
DROP JOB  load_USairport

CREATE LOADING JOB load_USairport FOR GRAPH USairport {
    DEFINE FILENAME vfile;
    DEFINE FILENAME efile;

    LOAD vfile
        TO VERTEX Airport VALUES ($0, $0, $1, $2, _)
        USING header="true";
    LOAD efile
        TO EDGE Flight VALUES ($0, $1, $2, $3, $4, $5, $6, $7, _)
        USING header="true";
        
}
#RUN LOADING JOB load_USairport USING dir_efile="./data/data.csv"
