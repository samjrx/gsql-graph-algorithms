DROP JOB  load_USairport
DROP GRAPH USairport
DROP EDGE Flight
DROP VERTEX Airport

CREATE VERTEX Airport (PRIMARY_ID id STRING, name STRING, City STRING, Position STRING, label STRING)
CREATE DIRECTED EDGE Flight (FROM Airport, TO Airport, Carrier STRING, Departures INT, Seats INT, Passengers INT, Aircraft STRING, Distance FLOAT, similarity FLOAT)
CREATE GRAPH USairport (Airport, Flight)
