DROP JOB  load_USairport
DROP GRAPH USairport
DROP EDGE Flight_to
DROP VERTEX Airport

CREATE VERTEX Airport (PRIMARY_ID id STRING, name STRING, City STRING, Position STRING, label STRING)
CREATE DIRECTED EDGE Flight_to (FROM Airport, TO Airport, weight FLOAT, Carrier STRING, Departures INT, Seats INT, Passengers INT, Aircraft STRING, Distance FLOAT, similarity FLOAT) WITH REVERSE_EDGE="Flight_from"
CREATE GRAPH USairport (Airport, Flight_to)
