DROP JOB  load_fw_baydry
DROP GRAPH fw_baydry
DROP EDGE Energy_to
DROP VERTEX Species

CREATE VERTEX Species (PRIMARY_ID id STRING, name STRING, ECO STRING, Biomass FLOAT, label STRING)
CREATE DIRECTED EDGE Energy_to (FROM Species, TO Species, weight FLOAT, similarity FLOAT) WITH REVERSE_EDGE="Energy_from"
CREATE GRAPH fw_baydry (Species, Energy_to)
