USE GRAPH wiki_vote
DROP QUERY ALL
DROP JOB ALL
DROP GRAPH wiki_vote

CREATE GRAPH wiki_vote ()
CREATE SCHEMA_CHANGE JOB schema_wiki_vote FOR GRAPH wiki_vote {
    ADD CREATE VERTEX Wiki_user (PRIMARY_ID id STRING, ID STRING);
    ADD DIRECTED EDGE Vote_for (FROM Wiki_user, TO Wiki_user, weight FLOAT) WITH REVERSE_EDGE="Voted_by";
}