DROP JOB  load_wiki_vote
DROP GRAPH wiki_vote
DROP EDGE Vote_for
DROP VERTEX Wiki_user

CREATE VERTEX Wiki_user (PRIMARY_ID id STRING, ID STRING, label STRING)
CREATE DIRECTED EDGE Vote_for (FROM Wiki_user, TO Wiki_user, weight FLOAT, similarity FLOAT) WITH REVERSE_EDGE="Voted_by"
CREATE GRAPH wiki_vote (Wiki_user, Vote_for)
