USE GRAPH netscience
DROP JOB  load_netscience

CREATE LOADING JOB load_netscience FOR GRAPH netscience {
    DEFINE FILENAME efile;

    LOAD efile
        TO VERTEX Scientist VALUES ($0, $0, _),
        TO EDGE Coauthorship VALUES ($0, $1, 1, _)
        USING header="true";
        
}
#RUN LOADING JOB load_netscience USING dir_efile="./data/data.csv"
